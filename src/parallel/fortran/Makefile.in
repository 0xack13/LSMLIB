SRC_DIR = @srcdir@
VPATH = @srcdir@
BUILD_DIR = @top_builddir@
include $(BUILD_DIR)/config/Makefile.config

# Set MAKE
@SET_MAKE@

all:  includes library

includes:  
	@CP@ $(SRC_DIR)/lsm_samrai_f77_utilities.h $(BUILD_DIR)/include/

library:  lsm_samrai_f77_utilities.o

clean:
		@RM@ *.o 

